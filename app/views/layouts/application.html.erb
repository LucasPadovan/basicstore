  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tag %>
</head>
<body id="store">
  <div id="banner" class="rainbow-background">
    <%= render partial: 'shared/locale_selector' %>
    <%= image_tag("CSA.png") %>
    <div id="titulo1">
      <!--<%= APP_CONFIG['public_domain'] %>-->
    </div>
  </div>
  <div id="layout" class="pure-g-r">
    <%#= render partial: 'shared/general_menu', params: params %>
    <div id="main" class="pure-u-7-8">
      <div class="content">
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>
